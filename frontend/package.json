{
	"name": "ccscan-frontend",
	"description": "A boilerplate to start a Nuxt+TS project quickly",
	"version": "0.1.0",
	"engine": "16",
	"type": "module",
	"private": true,
	"scripts": {
		"prepare": "cd .. && husky install frontend/.husky",
		"dev": "nuxt dev",
		"build": "nuxt build",
		"start": "nuxi start",
		"generate": "nuxt generate",
		"test": "jest",
		"test:watch": "jest --watch",
		"typecheck": "vue-tsc --noEmit",
		"lint": "eslint --fix --max-warnings 0 --ext .js,.ts,.vue .",
		"gql-codegen": "graphql-codegen --config graphql-codegen.yml"
	},
	"dependencies": {
		"@heroicons/vue": "^1.0.5",
		"@urql/devtools": "^2.0.3",
		"@urql/vue": "^0.6.1",
		"chart.js": "^3.7.1",
		"date-fns": "^2.27.0",
		"graphql": "^16.1.0",
		"nuxt3": "^3.0.0-27356801.e9128f3",
		"subscriptions-transport-ws": "^0.11.0",
		"vue": "^3.2.26"
	},
	"devDependencies": {
		"@babel/preset-typescript": "^7.16.0",
		"@graphql-codegen/cli": "2.5.0",
		"@graphql-codegen/typescript": "^2.4.3",
		"@nuxtjs/eslint-config-typescript": "8.0.0",
		"@testing-library/jest-dom": "^5.16.1",
		"@testing-library/vue": "^6.4.2",
		"@types/jest": "26",
		"@types/node": "^16.11.11",
		"@typescript-eslint/eslint-plugin": "^5.5.0",
		"@typescript-eslint/parser": "^5.5.0",
		"@vue/eslint-config-typescript": "^9.1.0",
		"autoprefixer": "^10.4.0",
		"babel-jest": "26",
		"eslint": "^8.3.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-nuxt": "^3.1.0",
		"eslint-plugin-vue": "^8.1.1",
		"firebase-admin": "^10.0.0",
		"firebase-functions": "^3.16.0",
		"firebase-functions-test": "^0.3.3",
		"husky": "^7.0.4",
		"jest": "26",
		"lint-staged": "^12.1.2",
		"postcss": "^8.4.4",
		"prettier": "^2.5.1",
		"tailwindcss": "^2.2.19",
		"ts-jest": "^26",
		"ts-node": "^10.4.0",
		"typescript": "^4.5.2",
		"vue-jest": "^5.0.0-alpha.10",
		"vue-tsc": "^0.30.2"
