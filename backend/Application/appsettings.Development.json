{
  "PostgresDatabase": {
    "ConnectionString" : "Host=localhost;Port=5432;Database=ConcordiumScan;"
  },
  "ConcordiumNodeGrpc": {
    "Address": "http://ftbccscandevnode.northeurope.cloudapp.azure.com:10111",
    "AuthenticationToken": "FTBgrpc2021"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate" : "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} [{Level:u4}] [{SourceClassName}] {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "c:\\logs\\concordiumscan_.log",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} [{Level:u4}] [{SourceClassName}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  }
}
